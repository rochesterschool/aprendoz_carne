<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.InscAlumCurso" table="Insc_Alum_Curso" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idInscAlumCurso" type="integer">
            <column name="Id_Insc_Alum_Curso"/>
            <generator class="identity"/>
        </id>
        <property name="calificacion" type="integer">
            <column name="Calificacion"/>
        </property>
        <property name="califChar" type="string">
            <column name="calif_char" length="10"/>
        </property>
        <property name="fechaMatricula" type="timestamp">
            <column name="fecha_matricula" length="19" not-null="true"/>
        </property>
        <many-to-one name="curso" class="com.aprendoz_desarrollodb.data.Curso">
            <column name="Curso_Id_Curso" not-null="true"/>
        </many-to-one>
        <many-to-one name="sy" class="com.aprendoz_desarrollodb.data.Sy">
            <column name="SY_Id_SY" not-null="true"/>
        </many-to-one>
        <many-to-one name="persona" class="com.aprendoz_desarrollodb.data.Persona">
            <column name="Persona_Id_Persona" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>