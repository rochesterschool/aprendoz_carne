<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.Nacionalidades" table="nacionalidades" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idNacionalidad" type="integer">
            <column name="id_nacionalidad"/>
            <generator class="identity"/>
        </id>
        <property name="nacionalidad" type="string">
            <column name="nacionalidad" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>