<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.DocentesVistaAsignaturas" table="docentes_vista_asignaturas" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.aprendoz_desarrollodb.data.DocentesVistaAsignaturasId">
            <key-property name="idAsignatura" type="integer">
                <column name="Id_Asignatura"/>
            </key-property>
            <key-property name="asignatura" type="string">
                <column name="Asignatura" length="300"/>
            </key-property>
            <key-property name="syIdSy" type="integer">
                <column name="SY_Id_SY"/>
            </key-property>
            <key-property name="gradoIdGrado" type="integer">
                <column name="Grado_Id_Grado"/>
            </key-property>
            <key-property name="electiva" type="boolean">
                <column name="Electiva"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>