<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.LogIngresoCarne" table="log_ingreso_carne" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idLogIngresoCarne" type="integer">
            <column name="id_log_ingreso_carne"/>
            <generator class="identity"/>
        </id>
        <property name="fechaCreacion" type="timestamp">
            <column name="fecha_creacion" length="19"/>
        </property>
        <property name="fechaActualizacion" type="timestamp">
            <column name="fecha_actualizacion" length="19"/>
        </property>
        <many-to-one name="tipoFalla" class="com.aprendoz_desarrollodb.data.TipoFalla">
            <column name="tipo_falla_id_tipo_falla" not-null="true"/>
        </many-to-one>
        <many-to-one name="persona" class="com.aprendoz_desarrollodb.data.Persona">
            <column name="persona_id_persona" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>