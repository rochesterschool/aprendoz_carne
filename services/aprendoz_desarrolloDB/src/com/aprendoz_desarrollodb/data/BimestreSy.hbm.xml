<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.BimestreSy" table="bimestre_sy" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idBimSy" type="integer">
            <column name="id_bim_sy"/>
            <generator class="identity"/>
        </id>
        <property name="bimestre" type="integer">
            <column name="bimestre" not-null="true"/>
        </property>
        <property name="fechaInicio" type="date">
            <column name="fecha_inicio" length="10"/>
        </property>
        <property name="fechaFin" type="date">
            <column name="fecha_fin" length="10"/>
        </property>
        <many-to-one name="sy" class="com.aprendoz_desarrollodb.data.Sy">
            <column name="sy_id_sy" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>