            <key-property name="aprProgreso" type="long">
                <column name="apr_progreso"/>
            </key-property>
            <key-property name="aprCompetente" type="long">
                <column name="apr_competente"/>
            </key-property>
            <key-property name="aprAvanzado" type="long">
                <column name="apr_avanzado"/>
            </key-property>
            <key-property name="aprMagistral" type="long">
                <column name="apr_magistral"/>
            </key-property>
            <key-property name="total" type="long">
                <column name="TOTAL"/>
            </key-property>
            <key-property name="sy" type="long">
