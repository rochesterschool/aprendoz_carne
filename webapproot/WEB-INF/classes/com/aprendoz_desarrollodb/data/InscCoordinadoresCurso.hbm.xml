<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.InscCoordinadoresCurso" table="insc_coordinadores_curso" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idInscCoordinadoresCurso" type="integer">
            <column name="id_insc_coordinadores_curso"/>
            <generator class="identity"/>
        </id>
        <property name="grupo" type="string">
            <column name="grupo" not-null="true"/>
        </property>
        <many-to-one name="curso" class="com.aprendoz_desarrollodb.data.Curso">
            <column name="curso_id_curso" not-null="true"/>
        </many-to-one>
        <many-to-one name="sy" class="com.aprendoz_desarrollodb.data.Sy">
            <column name="sy_id_sy" not-null="true"/>
        </many-to-one>
        <many-to-one name="persona" class="com.aprendoz_desarrollodb.data.Persona">
            <column name="persona_id_persona" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>