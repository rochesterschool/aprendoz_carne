<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.TestPorcentajes" table="test_porcentajes" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="testId" type="integer">
            <column name="test_id"/>
            <generator class="assigned"/>
        </id>
        <property name="descuentoPorcentaje" type="double">
            <column name="descuento_porcentaje" precision="22" not-null="true"/>
        </property>
        <property name="code" type="string">
            <column name="code" length="5" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>