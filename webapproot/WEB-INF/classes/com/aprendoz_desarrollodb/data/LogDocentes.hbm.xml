<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.LogDocentes" table="Log_Docentes" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idLogDocentes" type="integer">
            <column name="idLogDocentes"/>
            <generator class="identity"/>
        </id>
        <property name="fechaIngreso" type="date">
            <column name="Fecha_Ingreso" length="10" not-null="true"/>
        </property>
        <property name="horaIngreso" type="time">
            <column name="Hora_Ingreso" length="8" not-null="true"/>
        </property>
        <many-to-one name="persona" class="com.aprendoz_desarrollodb.data.Persona">
            <column name="Persona_Id_Persona" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>