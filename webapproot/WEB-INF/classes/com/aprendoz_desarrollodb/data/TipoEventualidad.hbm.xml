<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.TipoEventualidad" table="Tipo_Eventualidad" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idTipoEventualidad" type="integer">
            <column name="Id_Tipo_Eventualidad"/>
            <generator class="identity"/>
        </id>
        <property name="tipoEventualidad" type="string">
            <column name="Tipo_Eventualidad" length="45"/>
        </property>
        <set name="subtipoEventualidads" inverse="true" cascade="">
            <key>
                <column name="Tipo_Eventualidad_Id_Tipo_Eventualidad" not-null="true"/>
            </key>
            <one-to-many class="com.aprendoz_desarrollodb.data.SubtipoEventualidad"/>
        </set>
    </class>
</hibernate-mapping>