<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.AprLogrados" table="apr_logrados" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.aprendoz_desarrollodb.data.AprLogradosId">
            <key-property name="personaIdPersona" type="integer">
                <column name="Persona_Id_Persona"/>
            </key-property>
            <key-property name="fechaLogro" type="date">
                <column name="fecha_logro" length="10"/>
            </key-property>
            <key-property name="count___" type="long">
                <column name="`COUNT(*)`"/>
            </key-property>
            <key-property name="syIdSy" type="integer">
                <column name="SY_Id_SY"/>
            </key-property>
            <key-property name="idAsignatura" type="integer">
                <column name="Id_Asignatura"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>