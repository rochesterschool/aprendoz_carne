<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.Personal" table="PERSONAL" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="codigo" type="string">
            <column name="Codigo"/>
            <generator class="assigned"/>
        </id>
        <property name="noDocumento" type="string">
            <column name="No_Documento" not-null="true"/>
        </property>
        <property name="rh" type="string">
            <column name="RH" not-null="true"/>
        </property>
        <property name="eps" type="string">
            <column name="EPS" not-null="true"/>
        </property>
        <property name="afp" type="string">
            <column name="AFP" not-null="true"/>
        </property>
        <property name="arp" type="string">
            <column name="ARP" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>